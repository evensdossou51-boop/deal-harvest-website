# Environment Variables for Deal Harvest
# Copy this file to .env and fill in your actual API credentials

# eBay API Credentials (from https://developer.ebay.com/)
EBAY_APP_ID=your_app_id_here
EBAY_DEV_ID=your_dev_id_here  
EBAY_CERT_ID=your_cert_id_here

# eBay Partner Network (from https://partnernetwork.ebay.com/)
EBAY_CAMPAIGN_ID=your_campaign_id_here
EBAY_AFFILIATE_ID=your_affiliate_id_here

# Walmart API (if using)
WALMART_API_KEY=your_walmart_api_key_here
WALMART_AFFILIATE_ID=your_walmart_affiliate_id_here